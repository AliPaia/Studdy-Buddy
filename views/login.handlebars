<header class="text-center credential-format">

  <header class='container text-center'></header>
  <header class='container text-center'></header>

  <form>
    <div>
      <section id='error-warning'></section>
      <label>Username:</label>
      <input id='user-create' type='text' name='name' />
      <label>Password</label>

      <input
        data-bs-toggle='tooltip'
        data-bs-title='Password must be at least 8 characters in length'
        id='password-create'
        type='password'
        name='password'
      />
      <button id='create-account' class='btn btn-primary btn-grad'>Create
        Account</button>
        </div>
  </form>

  
  <header class='container text-left'></header>
  <header class='container text-right'></header>

  <form>
    <div>
      <label>Username:</label>
      <input id='username' type='text' name='name' />
      <label>Password</label>
      <input id='password' type='password' name='password' />
      <button id='profile-login' class='btn btn-primary btn-grad'>Go to Profile</button>
      </div>
  </form>

  <header class='container text-left'></header>
  <header class='container text-right'></header>

</header>

{{#section 'script'}}
  <script src='/js/login.js'></script>
{{/section}}